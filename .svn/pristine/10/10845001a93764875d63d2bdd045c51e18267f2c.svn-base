package com.habebe.projecthomework.database;

public class EXAMINATION_TABLE {
    public static final String TABLE = "EXAMINATION_TABLE";
    public static final String EXAMID = "EXAMID";
    public static final String CHAPTERID = "SUBJECTID";
    public static final String DESCRIPTION = "DESCRIPTION";
    public static final String EXAMDATE = "EXAMDATE";
    public static final String ADDEVENTED = "ADDEVENTED";

    public static String onCreate() {
        String DATABASE_CREATE = "create table "
                + TABLE
                + " ("
                + "_id" + " TEXT, "
                + EXAMID + " TEXT, "
                + CHAPTERID + " TEXT, "
                + ADDEVENTED + " TEXT, "
                + DESCRIPTION + " TEXT, "
                + EXAMDATE + " TEXT);";
        return DATABASE_CREATE;
    }

}
